{
  "name": "safepay-ai",
  "version": "1.0.0",
  "description": "SafePay AI - Decentralized Expense & Privacy Guardian",
  "main": "src/index.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "cross-env NODE_ENV=production tsc && webpack --mode production",
    "build:dev": "tsc && webpack",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon src/server/index.ts",
    "client": "webpack serve --config webpack.config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "echo 'Linting not configured yet'"
  },
  "keywords": [
    "decentralized",
    "privacy",
    "AI",
    "finance"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "ethers": "^6.9.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^3.3.0",
    "tesseract.js": "^6.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.8.2",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "concurrently": "^8.2.0",
